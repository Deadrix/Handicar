<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Bienvenue{% endblock %} | Handicar</title>
        <link rel="icon" href="{{ asset("/images/icon.ico") }}">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body class="flex w-screen h-screen items-center justify-center">
        {% block body %}{% endblock %}
    </body>
</html>
